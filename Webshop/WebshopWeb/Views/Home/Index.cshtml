@using WeatherAPI.Model
@using Webshop.Authenticator.Services.Authenticator
@model AuthenticatorService
@{
    ViewData["Title"] = "Home Page";
    var isAuthenticated = ViewBag.IsAuthenticated;
}

<input type="text" id="search" placeholder="Keresés" alt="Keresés"/>
<div class="text-center">
    <h1 class="display-4">Üdvözlünk a webshopon!</h1>
    @if(isAuthenticated==true)
    {
        <h1>W</h1>
    }
    else
    {
        <h1>L</h1>
    }
</div>
<table>
    <thead>
        <th>Város</th>
        <th>Időjárás</th>
        <th>Leírás</th>

        
    </thead>
    <tbody>
        <td></td>
        <td></td>
        <td></td>
    </tbody>
</table>
